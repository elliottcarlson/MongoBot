<script>
                var pageNum = 1;
                // CALLBACK FUNCTION AFTER LOAD ELEMENTS WITH INFINITE SCROLL
                function infiniteCallback() {
                    console.log("infiniteCallback");
                    ga('send', 'pageview', location.pathname+'/scroll_'+pageNum);
                    pageNum++;
                    $(this).hide().fadeIn(1000);
                    prepareFlipCard(this);
                    articleVoteActions(this);
                    sharrebuttons(this);
                    articleBookmarkActions(this);
                    inittaboola(this);
                    smileysVote(this);
                    commentActions(this);
                    console.log("END-infiniteCallback");
                }

                $( document ).ready(function() {
                    $('.infinite-scroll').jscroll({
                        loadingHtml: '<div class="text-center"> <i class="fa fa-spinner fa-pulse fa-fw"></i> Loading...</div>',
                        padding: 20,
                        nextSelector: 'a.jscroll-next:last',
                        contentSelector: '.jscroll-inner',
                        //debug: true,
                        //autoTrigger: false
                        autoTriggerUntil: 1,
                        callback: infiniteCallback
                    });
                });

                // TABOOLA

                var taboola_timer = 0;

                function inittaboola(element){

                    console.log("inittaboola");

                    var content = element || "body";
                    $(content).find('[data-taboola]').each(function(){
                        var container_id = $(this).data('taboola');
                        var taboolamode = $(this).data('taboolamode');
                        setTimeout(function() {
                            //alert(container_id)
                            lunchtaboola(container_id,taboolamode);
                        }, taboola_timer);
                        taboola_timer = taboola_timer + 300;

                    });

                    taboola_timer = 0;
                }

                function lunchtaboola(container_id,taboolamode){
                    window._taboola = window._taboola || [];
                    _taboola.push({
                        mode: taboolamode,
                        container: container_id,
                        placement: 'Mid Article Thumbnails',
                        target_type: 'mix'
                    });
                }

            </script>
<div class="col-xs-12 block clearfix" id="taboola-mid-article-thumbnails-3"></div>
<div class="clearfix"></div>
<script type="text/javascript">
    window._taboola = window._taboola || [];
    _taboola.push({
        mode: 'thumbnails-a',
        container: 'taboola-mid-article-thumbnails-3',
        placement: 'Page bottom',
        target_type: 'mix'
    });
</script>
</div>
