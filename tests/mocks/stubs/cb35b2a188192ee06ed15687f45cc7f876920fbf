<script>

    function hidepopup() {
        $("[data-toggle='popover']").popover('hide');
    }

    function popupAccountNeed(){

        var modalAccount = {
            "content": [
                {
                    "title": "You must register to do this! It costs nothing and it&#039;s gluten-free.",
                    "body": "<div><img class=\"img-responsive\" src=\"/images/gifs/matrix.gif\"></div>"
                }
            ],
            "buttons": [
                {
                    "text": "Log in",
                    "link": "/login",
                    "btntype": "btn-primary"
                },
                {
                    "text": "Join the FML community",
                    "link": "/user/add",
                    "btntype": "btn-primary"
                }
            ]
        };
        lunchPopup(modalAccount);

    }

    function popupConfirmAction(){

        var modalConfirmAction = {
            "content": [
                {
                    "title": "Your action is ok",
                    "body": "<div>Please reload the page to see result</div>"
                }
            ],
            "buttons": [
                {
                    "text": "Close",
                    "calback": "",
                    "btntype": "btn-default"
                }
            ]
        };
        lunchPopup(modalConfirmAction);

    }

    function lunchPopup(modalContent){

        var modal = $('#modalMessage');
        modal.find('.modal-footer').html('');

        $.each(modalContent.buttons, function(index, value) {
            var callback = "";
            if(value.calback != ""){
                callback = "onclick=\""+value.calback+"\"";
            }

            if(value.link != "" && typeof value.link != 'undefined'){
                callback = "onclick=\"gotto('"+value.link+"');\"";
                //callback = "onclick=\"alert('toto');\"";
            }

            var addbutton = "<button "+callback+" type=\"button\" class=\"btn "+value.btntype+"\" data-dismiss=\"modal\">"+value.text+"</button>";
            modal.find('.modal-footer').append(addbutton);
        });

        if (typeof modalContent.content[0].class != 'undefined'){
            var modalclass = modalContent.content[0].class;
            modal.addClass(modalclass);
        }

        modal.find('.modal-title').html(modalContent.content[0].title);
        modal.find('.modal-body').html(modalContent.content[0].body);
        modal.modal();
    }

    function gotto(url){
        $(location).attr('href', url);
        return false;
    }

    </script>
